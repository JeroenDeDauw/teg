#: robot/ai_msg.c:65
msgid "I'm a bit tired"
msgstr "Chyba muszę się przespać"

#: robot/ai_msg.c:66
msgid "I'm hungry"
msgstr "Muszę coś zjeść"

#: robot/ai_msg.c:67
msgid "Who wants an Empanada ?"
